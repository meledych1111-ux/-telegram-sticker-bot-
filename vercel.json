{
  "version": 2,
  "builds": [
    {
      "src": "api/bot.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/bot.js"
    }
  ],
  "functions": {
    "api/bot.js": {
      "runtime": "nodejs24.x",
      "maxDuration": 60,
      "memory": 3008
    }
  }
}
