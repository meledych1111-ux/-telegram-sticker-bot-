{
  "functions": {
    "api/bot.js": {
      "maxDuration": 10,
      "runtime": "nodejs20.x"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/bot"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store"
        }
      ]
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
