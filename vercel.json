{
  "version": 2,
  "builds": [
    {
      "src": "api/bot.js",
      "use": "@vercel/node",
      "config": {
        "runtime": "nodejs24.x"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/bot"
    },
    {
      "src": "/",
      "dest": "/api/bot"
    }
  ]
}
