{
  "version": 2,
  "builds": [
    {
      "src": "api/bot.js",
      "use": "@vercel/node",
      "config": {
        "runtime": "nodejs20.x"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/bot.js"
    }
  ],
  "functions": {
    "api/bot.js": {
      "runtime": "nodejs20.x",
      "maxDuration": 60,
      "memory": 3008
    }
  }
}
