{
  "version": 2,
  "builds": [
    {
      "src": "api/bot.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/bot",
      "dest": "/api/bot.js",
      "methods": ["POST", "GET"]
    },
    {
      "src": "/api/check-env",
      "dest": "/api/bot.js",
      "methods": ["GET"]
    },
    {
      "src": "/(.*)",
      "dest": "/api/bot.js"
    }
  ]
}
